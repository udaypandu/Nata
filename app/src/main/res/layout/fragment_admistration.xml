                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom|center"
                    android:text="@string/videos"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_16sp" />

            </LinearLayout>

            <LinearLayout

                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txt_view_contents_icon"
                    android:layout_width="@dimen/_60dp"
                    android:layout_height="@dimen/_60dp"
                    android:background="@drawable/white_circle_with_red_strok"
                    android:gravity="center"
                    android:text="@string/view_contents_icon"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_40sp" />

                <TextView
                    android:id="@+id/txt_view_contents"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom|center"
                    android:text="@string/view_contents"
                    android:textColor="@color/very_dark_gray"
                    android:textSize="@dimen/_16sp" />

                <TextView
                    android:id="@+id/txt_view_contents_news"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom|center"
                    android:text="@string/news"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_16sp" />

                <TextView
                    android:id="@+id/txt_view_contents_events"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom|center"
                    android:text="@string/events"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_16sp" />

                <TextView
                    android:id="@+id/txt_view_contents_videos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="bottom|center"
                    android:text="@string/videos"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_16sp" />
            </LinearLayout>
        </LinearLayout>

        <TextView

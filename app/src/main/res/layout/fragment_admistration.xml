                        android:text="@string/pending_registration_icon"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/_40sp" />

                    <TextView
                        android:id="@+id/txt_pending_registration"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="bottom|center"
                        android:text="@string/pending_registration"
                        android:textColor="@color/very_dark_gray"
                        android:textSize="@dimen/_16sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="@dimen/_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txt_under_review_icon"
                        android:layout_width="@dimen/_60dp"
                        android:layout_height="@dimen/_60dp"
                        android:background="@drawable/white_circle_with_red_strok"
                        android:gravity="center"
                        android:text="@string/registered_users_icon"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/_40sp" />

                    <TextView
                        android:id="@+id/txt_under_review"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="bottom|center"
                        android:text="@string/under_review"
                        android:textColor="@color/very_dark_gray"
                        android:textSize="@dimen/_16sp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <TextView

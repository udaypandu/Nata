                        android:layout_height="@dimen/_60dp"
                        android:background="@drawable/white_circle_with_red_strok"
                        android:gravity="center"
                        android:text="@string/add_user_icon"
                        android:textColor="@color/colorPrimary"
                        android:textSize="@dimen/_40sp" />

                    <TextView
                        android:id="@+id/txt_add_user"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="bottom|center"
                        android:text="@string/add_user"
                        android:textColor="@color/very_dark_gray"
                        android:textSize="@dimen/_16sp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="@dimen/_0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txt_role_change_icon"
                        android:layout_width="@dimen/_60dp"
                        android:layout_height="@dimen/_60dp"
                        android:background="@drawable/white_circle_with_red_strok"
                        android:gravity="center"
                        android:text="@string/role_change_icon"

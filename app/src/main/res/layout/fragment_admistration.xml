            android:layout_height="@dimen/_40dp"
            android:background="@color/color_hexa"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/_10dp"
            android:text="@string/sms_email"
            android:textColor="@color/very_dark_gray"
            android:textSize="@dimen/_18sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/_10dp"
            android:paddingTop="@dimen/_10dp">

            <LinearLayout
                android:layout_width="@dimen/_0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txt_group_icon"
                    android:layout_width="@dimen/_60dp"
                    android:layout_height="@dimen/_60dp"
                    android:background="@drawable/white_circle_with_red_strok"
                    android:gravity="center"
                    android:text="@string/group_icon"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/_40sp" />

                <TextView
                    android:id="@+id/txt_group"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
